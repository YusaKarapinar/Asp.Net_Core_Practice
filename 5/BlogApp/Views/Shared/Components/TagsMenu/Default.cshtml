@model List<Tag>
<div class="col-lg-4 ">
    <div class="bg-white p-4 rounded">
        <h2 class="h5 mb-4">Categories</h2>
        <ul class="list-unstyled ">
            @foreach(var tag in @Model)
            {
            <li><a href="#" class="text-decoration-none">@tag.TagName</a></li>
            }
        </ul>

    </div>
</div>